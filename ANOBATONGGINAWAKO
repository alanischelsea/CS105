        public Form1()
        {
            InitializeComponent();
        }

        public void showInfo(int idnum, string n) 
        {
            labelApply.Text = " applicant " + idnum + ": " + n;
                                 
            
        }

        private void buttonApply_Click(object sender, EventArgs e)
        {
            Apply form = new Apply();
            form.applyForm = this;
            form.ShowDialog();
        }

        private void buttonLine_Click(object sender, EventArgs e)
        {
            LineUp form = new LineUp();
            form.lineForm = this;
            form.ShowDialog();
        }

